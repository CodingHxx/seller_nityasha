{
  "expo": {
    "name": "N Partner",
    "slug": "n-partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nityashapartner",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e0e0e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.NityashaPartner",
      "versionCode": 1
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#000000"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "703e961c-7bbf-43f9-99e4-5bc60b4b8eb7"
      }
    },
    "owner": "option_services"
  },
  "reactNativeDirectoryCheck": {
    "listUnknownPackages": false,
    "exclude": [
      "@expo-google-fonts/urbanist",
      "firebase",
      "pusher-js"
    ]
  }
}
